# Backend Environment Variables
# Copy this file to .env and fill in the actual values

# =================================
# Server Configuration
# =================================

# Port number for the backend server
PORT=3001

# Node environment (development, production, test)
NODE_ENV=development

# Base URL for generating short links (should match your deployed backend URL)
BASE_URL=http://localhost:3001

# =================================
# Frontend Configuration
# =================================

# Frontend URL for CORS configuration
FRONTEND_URL=http://localhost:3000

# =================================
# Supabase Configuration
# =================================

# Supabase project URL - get this from your Supabase dashboard
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase anonymous key - get this from your Supabase dashboard -> Settings -> API
SUPABASE_ANON_KEY=your-supabase-anon-key-here

SUPABASE_SERVICE_ROLE_KEY=key
